<?xml version="1.0" encoding="utf-8" ?>
<prism:PrismApplication xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="clr-namespace:Prism.Unity;assembly=Prism.Unity.Forms"
             x:Class="EasySoccer.Mobile.Adm.App">
    <Application.Resources>
        <ResourceDictionary>
            <Style x:Key="fontAwesomeLabel" TargetType="Label">
                <Setter Property="FontFamily" Value="fa-solid-900.ttf#Font Awesome 5 Free Solid" />
            </Style>
            <Style x:Key="gilroyLabel" TargetType="Label">
                <Setter Property="FontFamily" Value="Gilroy-ExtraBold.otf#Gilroy-ExtraBold" />
            </Style>
            <Style x:Key="gilroyLightLabel" TargetType="Label">
                <Setter Property="FontFamily" Value="Gilroy-Light.otf#Gilroy-Light" />
            </Style>
            <Style x:Key="gilroyLightEntry" TargetType="Entry">
                <Setter Property="FontFamily" Value="Gilroy-Light.otf#Gilroy-Light" />
            </Style>
            <Style x:Key="gilroyLightButtonPrimary" TargetType="Button">
                <Setter Property="FontFamily" Value="Gilroy-Light.otf#Gilroy-Light" />
                <Setter Property="BackgroundColor" Value="#161E53" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>